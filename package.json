    "@sveltejs/enhanced-img": "^0.4.4",
    "@types/bun": "^1.2.2",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.13.1",
    "mdsvex": "^0.12.3",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
